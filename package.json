{
  "name": "duck-duck-tag",
  "version": "0.1.0",
  "description": "Tool for using multiple image recognition services ",
  "author": "duck-duck-tag",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=dev webpack-dev-server --mode=development",
    "build": "webpack --mode=production",
    "electron-rebuild": "electron-rebuild --parallel --force --types prod,dev --module-dir app ",
    "package-linux": "npm run build && electron-builder build --linux",
    "package-win": "electron-builder build --win",
    "package-mac": "electron-builder build --mac",
    "postinstall": "electron-rebuild --install-app-deps",
    "test": "jest",
    "test-e2e": "NODE_ENV=test testcafe electron:./app ./tests/e2e/Menu.e2e.js ./tests/e2e/TagImages.e2e.js",
    "start": "electron ./app/main.js"
  },
  "build": {
    "productName": "Siipikarjalautaspohja",
    "appId": "org.secondArgument.siipikarjalautaspohja",
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Science"
    },
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": "x64"
        },
        {
          "target": "msi",
          "arch": "x64"
        }
      ]
    },
    "directories": {
      "buildResources": "resources"
    }
  },
  "keywords": [
    "are_there_ducks?,",
    "whose_AI_are_you?"
  ],
  "jest": {
    "projects": [
      {
        "displayName": "jest-electron-runner tests",
        "runner": "@jest-runner/electron/main",
        "testEnvironment": "node",
        "testMatch": [
          "<rootDir>/tests/native/exportResults.native.test.js"
        ],
        "moduleDirectories": [
          "node_modules",
          "app/node_modules"
        ]
      },
      {
        "displayName": "non-native tests",
        "testPathIgnorePatterns": [
          "/node_modules/",
          "/native/"
        ],
        "moduleNameMapper": {
          "\\.(css)$": "identity-obj-proxy"
        },
        "moduleFileExtensions": [
          "js",
          "jsx",
          "json"
        ]
      }
    ]
  },
  "dependencies": {
    "axios": "^0.19.2",
    "crypto-js": "^4.0.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.27.0",
    "react": "^16.13.1",
    "react-bootstrap": "^1.3.0",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "react-wordcloud": "^1.2.7",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/polyfill": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@jest-runner/electron": "^3.0.0",
    "@testing-library/jest-dom": "^5.11.2",
    "@testing-library/react": "^10.4.7",
    "babel-jest": "^26.2.2",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.6.0",
    "electron": "^11.5.0",
    "electron-builder": "^22.8.0",
    "electron-devtools-installer": "^3.1.1",
    "electron-react-devtools": "^0.5.3",
    "electron-rebuild": "^1.11.0",
    "html-webpack-plugin": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.2.2",
    "react-test-renderer": "^16.13.1",
    "redux-devtools-extension": "^2.13.8",
    "style-loader": "^1.2.1",
    "testcafe": "^1.9.1",
    "testcafe-browser-provider-electron": "0.0.15",
    "webpack": "^5.0.0-beta.22",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
